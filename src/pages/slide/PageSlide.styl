    .lwSlide
      #homepage
        .figure
          top 56px
        .msgBox
          bottom 18%
      #canvas1
        top 118px
      #canvas2
        top 118px
      #canvas3
        top 118px
      #lastPage
        .figure
          top 65px
        .msgBox
          bottom 18%

